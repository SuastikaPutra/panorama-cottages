{
    "version": 2,
    "builds": [
      {
        "src": "api/index.php",
        "use": "@vercel-php@0.7.3"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/favicon.ico",
        "dest": "/public/favicon.ico"
      },
      {
        "src": "/build/(.*)",
        "dest": "/public/build/$1"
      },
      {
        "src": "/storage/(.*)",
        "dest": "/public/storage/$1"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/index.php"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://panorama-cottages.vercel.app",
      "APP_KEY": "base64:G9A6+2iucCvi2yw4iqRcFARgQV4ALDhic/igVhzvBRg=",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie",
      "CACHE_DRIVER": "array",
      "VIEW_COMPILED_PATH": "/tmp",
      "APP_STORAGE_PATH": "/tmp/storage"
    }
  }